# Define two vectors as input
vector1 <- c(1, 2, 3, 4, 5, 6, 7, 8, 9)
vector2 <- c(10, 11, 12, 13, 14, 15, 16, 17, 18)

# Combine vectors into a list
input_list <- list(vector1, vector2)

# Create the array
array_data <- array(unlist(input_list), dim = c(3, 3, 2))

# Print the array
print(array_data)
